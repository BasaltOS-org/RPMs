[baseos]
name=BasaltOS $releasever - BaseOS
mirrorlist=https://mirrors.almalinux.org/mirrorlist/$releasever/baseos
# baseurl=https://kitten.repo.almalinux.org/$releasever-kitten/BaseOS/$basearch/os/
enabled=1
gpgcheck=1
countme=1
exclude=almalinux*
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Basalt-10
metadata_expire=86400
enabled_metadata=1

[baseos-debuginfo]
name=BasaltOS $releasever - BaseOS - Debug
mirrorlist=https://mirrors.almalinux.org/mirrorlist/$releasever/baseos-debug
# baseurl=https://kitten.vault.almalinux.org/$releasever-kitten/BaseOS/debug/$basearch/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Basalt-10
metadata_expire=86400
enabled_metadata=0

[baseos-source]
name=BasaltOS $releasever - BaseOS - Source
mirrorlist=https://mirrors.almalinux.org/mirrorlist/$releasever/baseos-source
# baseurl=https://kitten.vault.almalinux.org/$releasever-kitten/BaseOS/Source/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Basalt-10
metadata_expire=86400
enabled_metadata=0

